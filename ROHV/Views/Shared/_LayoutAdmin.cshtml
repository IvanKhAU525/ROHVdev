@using ROHV.Core.User;
@{ 
    var CurentUser = (CurrentUserModelView)ViewData["CurrentUser"];
    var contact = (string)ViewData["loggedContact"];
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Rayim, web application</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    <link rel="shortcut icon" type="image/x-icon" href="~/favicon.ico">
    <link rel="icon" type="image/png" href="~/favicon.png">

    <link rel="stylesheet" href="~/Scripts/libs/bootstrap/dist/css/bootstrap.css" type="text/css" />
    <link rel="stylesheet" href="~/Scripts/libs/font-awesome/css/font-awesome.min.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/admin/admin.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/admin/admin-additional.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/admin/animate.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/admin/skin-black-light.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/admin/loading.css" type="text/css" />

    <link rel="stylesheet" href="~/Scripts/libs/datatables.net-bs/css/dataTables.bootstrap.css" />
    <link rel="stylesheet" href="~/Scripts/libs/datatables.net-responsive-bs/css/responsive.bootstrap.css" />
    <link rel="stylesheet" href="~/Scripts/libs/datatables.net-select-bs/css/select.bootstrap.css" />

    <link rel="stylesheet" href="~/Scripts/libs/bootstrap-select/dist/css/bootstrap-select.css" />
    <link rel="stylesheet" href="~/Scripts/libs/ajax-bootstrap-select/dist/css/ajax-bootstrap-select.css" />

    <link rel="stylesheet" href="~/Scripts/libs/bootstrap-switch/dist/css/bootstrap3/bootstrap-switch.min.css" />
    <link rel="stylesheet" href="~/Scripts/libs/bootstrap-datepicker/dist/css/bootstrap-datepicker3.min.css" />
    <link rel="stylesheet" href="~/Scripts/libs/bootstrap-datepicker/dist/css/bootstrap-datepicker3.standalone.min.css" />

    <link rel="stylesheet" type="text/css" href="~/Scripts/libs/dropzone/dist/basic.css">
    <link rel="stylesheet" type="text/css" href="~/Scripts/libs/dropzone/dist/dropzone.css">

    <link rel="stylesheet" href="@Request.Url.Scheme://fonts.googleapis.com/css?family=Archivo+Black" type="text/css" />
    <link rel="stylesheet" href="@Request.Url.Scheme://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic" type="text/css" />
    <link rel="stylesheet" href="@Request.Url.Scheme://fonts.googleapis.com/css?family=Montserrat:400,700" type="text/css" />
    <link href='@Request.Url.Scheme://fonts.googleapis.com/css?family=Open+Sans&subset=latin,cyrillic,latin-ext' rel='stylesheet' type='text/css'>

    @RenderSection("head", required: false)
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="hold-transition skin-black-light sidebar-mini fixed overflowhidden">
    <div class="wrapper">
        
        <!-- Main Header -->
        @Html.Partial("Layouts/_LayoutPartialHeader")
        <!-- Left side column. contains the logo and sidebar -->
        @Html.Partial("Layouts/_LayoutPartialNav")

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">     
            @RenderBody()
        </div><!-- /.content-wrapper -->

        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div><!-- ./wrapper -->

    @Html.Partial("Layouts/_LayoutPartialError")
    @Html.Partial("Layouts/_LayoutPartialLoading")
    @Html.Partial("Layouts/_LayoutPartialNotFound")
       
    <script src="~/Scripts/libs/requirejs/require.js" data-main="/Scripts/application/loader.js"></script>
    <script type="text/javascript">        
        var CurrentUserInfo = { Id:@CurentUser.UserId, Name: "@CurentUser.Name" };        
        var ContactInfo = JSON.parse('@Html.Raw(@""+ (!String.IsNullOrEmpty(contact) ? contact :  "{}") + "")');            
    </script>
</body>
</html>